# ===================================================================
# 量化研究系统 - 环境变量配置
# 复制此文件为 .env 并填入实际值
# ===================================================================

# ==================== 数据源配置 ====================

# Tushare API Token（必填，用于数据同步）
# 获取地址: https://tushare.pro/register
TUSHARE_TOKEN=your_tushare_token_here

# API 限流配置（每分钟调用次数）
COLLECTOR__CALLS_PER_MINUTE=120

# ==================== DolphinDB 配置 ====================

# DolphinDB 连接信息
DOLPHINDB_HOST=localhost
DOLPHINDB_PORT=8848
DOLPHINDB_USER=admin
DOLPHINDB_PASSWORD=123456
DOLPHINDB_DB_PATH=dfs://quant_research
DOLPHINDB_META_DB_PATH=dfs://quant_meta

# ==================== Prefect 配置 ====================

# Prefect Server API 地址
PREFECT_API_URL=http://localhost:4200/api

# ==================== 回测配置 ====================

# 初始资金（元）
BACKTEST__INITIAL_CAPITAL=1000000

# 手续费率
BACKTEST__COMMISSION_RATE=0.0003

# 滑点率
BACKTEST__SLIPPAGE_RATE=0.0001

# ==================== 应用配置 ====================

# 运行环境 (development/production)
ENVIRONMENT=development

# 调试模式
DEBUG=false

# 日志级别 (DEBUG/INFO/WARNING/ERROR)
LOG_LEVEL=INFO

# ==================== 前端配置 ====================

# API 基础地址
REACT_APP_API_BASE_URL=http://localhost:8000

# Prefect UI 地址（前端 iframe 嵌入）
REACT_APP_PREFECT_URL=http://localhost:4200

# ==================== 安全配置 ====================

# JWT 密钥（生产环境必须修改）
SECRET_KEY=your-secret-key-change-in-production

# CORS 允许的源（逗号分隔）
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
